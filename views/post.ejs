<script>
$(".navmenu").attr({src:"https://sangamnayak.github.io/raxmiceportfolio/public/img/menu.svg" , alt:"menu"});
let objj = document.cookie;
let strr = objj.replaceAll('=', ':');
let arrr = strr.split(" ");
console.log(arrr);
    arrr.forEach(cookie => {
      if(cookie === "mode:dark"){ dark();
            $("#mode,.navmode").attr({src:"https://sangamnayak.github.io/raxmiceportfolio/public/img/sun.svg" , alt:"sun"});     
        }
      else{ light();
            $("#mode,.navmode").attr({src:"https://sangamnayak.github.io/raxmiceportfolio/public/img/moon.svg" , alt:"moon"}); }
    });
    function mode(){
        var y = $("#mode"), x = y.attr("alt");
            if (x == "sun") {
                light();
            $("#mode,.navmode").attr({src:"https://sangamnayak.github.io/raxmiceportfolio/public/img/moon.svg" , alt:"moon"});
            } else {
                dark();
            $("#mode,.navmode").attr({src:"https://sangamnayak.github.io/raxmiceportfolio/public/img/sun.svg" , alt:"sun"});
            }
        }
    
        function menu(){
        var y = $(".navmenu"), x = y.attr("alt");
        if (x == "menu") {
        $(".navmenu").attr({src:"https://sangamnayak.github.io/raxmiceportfolio/public/img/close.svg" , alt:"close"});
        $(".menus").css({display:"flex", transition: "5s"});
        } else {
        $(".navmenu").attr({src:"https://sangamnayak.github.io/raxmiceportfolio/public/img/menu.svg" , alt:"menu"});
        $(".menus").css("display","none");
        }
    }
</script>
